@model IEnumerable<Grievancemis.Models.GrivanceModel>

<strong>Grievance </strong>
<div class="row row-deck">
    <div class="col-lg-12 col-md-12">
        <div class="card">
            <div class="table-responsive">
                <table class="table table-bordered table-hover dt-responsive display tbl" id="Tbl_Grievance" width="100%">
                    <thead>
                        <tr>
                            <th>Id</th>
                            <th>Email</th>
                            <th>Name</th>
                            <th>Grievance Type</th>
                            <th>State</th>
                            <th>Location</th>
                            <th>Title</th>
                            <th>Grievance Message</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>@item.Id</td>
                                <td>@item.Email</td>
                                <td>@item.Name</td>
                                <td>@item.GrievanceType</td>
                                <td>@item.StateId</td>
                                <td>@item.Location</td>
                                <td>@item.Title</td>
                                <td>@item.GrievanceMessage</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>